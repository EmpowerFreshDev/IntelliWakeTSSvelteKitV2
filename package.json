{
	"name": "@solidbasisventures/intelliwaketssveltekitv2",
	"version": "2.0.33",
	"license": "MIT",
	"private": false,
	"scripts": {
		"dev": "vite dev",
		"Publish": "pnpm version patch && git push --tags && pnpm svelte-package && pnpm publish && git push",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"Install-IntelliWake": "pnpm install && pnpm add @solidbasisventures/intelliwaketsfoundation@latest",
		"Version-Minor-Advance": "pnpm version minor"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@solidbasisventures/intelliwaketsfoundation": "^5.11.8",
		"@sveltejs/kit": "^2.4.1",
		"svelte": "^4.2.9"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-common-types": "^6.5.1",
		"@sveltejs/adapter-auto": "^3.1.0",
		"@sveltejs/package": "^2.2.5",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"postcss-nesting": "^12.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.16",
		"svelte-check": "^3.6.2",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.10",
		"vite-plugin-mkcert": "^1.17.1",
		"vitest": "^1.1.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}

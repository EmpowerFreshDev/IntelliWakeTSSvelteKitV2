{
  "name": "@empowerfresh/intelliwake-ui",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "description": "EmpowerFresh UI components forked from @solidbasisventures/intelliwaketssveltekitv2",
  "repository": {
    "type": "git",
    "url": "https://github.com/EmpowerFreshDev/intelliwaketssveltekitv2.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest --watch",
    "vitest": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "version:minor": "pnpm version minor",
    "publish:local": "pnpm version patch && git push --tags && pnpm run package && git push"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@empowerfreshdev/intelliwaketsfoundation": "github:EmpowerFreshDev/intelliwaketsfoundation#main",
    "@sveltejs/kit": "^2.5.18",
    "svelte": "^4.2.18"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-common-types": "^6.5.1",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/package": "^2.3.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.42.0",
    "postcss": "^8.4.39",
    "postcss-nesting": "^12.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "publint": "^0.2.7",
    "svelte-check": "^3.8.4",
    "svelte-preprocess": "^5.1.4",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.3.3",
    "vite-plugin-mkcert": "^1.17.5",
    "vitest": "^2.0.2"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}

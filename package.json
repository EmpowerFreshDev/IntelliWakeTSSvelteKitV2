{
	"name": "@solidbasisventures/intelliwaketssveltekitv2",
	"version": "2.0.77",
	"license": "MIT",
	"private": false,
	"scripts": {
		"dev": "vite dev",
		"Publish": "pnpm version patch && git push --tags && pnpm svelte-package && pnpm publish && git push",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest --watch",
		"vitest": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"Install-IntelliWake": "pnpm install && pnpm add @solidbasisventures/intelliwaketsfoundation",
		"Version-Minor-Advance": "pnpm version minor"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@solidbasisventures/intelliwaketsfoundation": "^5.12.24",
		"@sveltejs/kit": "^2.5.18",
		"svelte": "^4.2.18"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-common-types": "^6.5.1",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/package": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"svelte-preprocess": "^5.1.4",
		"eslint-plugin-svelte": "^2.42.0",
		"postcss": "^8.4.39",
		"postcss-nesting": "^12.1.5",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"publint": "^0.2.7",
		"svelte-check": "^3.8.4",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.3.3",
		"vite-plugin-mkcert": "^1.17.5",
		"vitest": "^2.0.2"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
